<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.cell.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.collections.*?>
<?import fxmltableview.*?>

<AnchorPane id="AnchorPane" prefHeight="476.0" prefWidth="715.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="schedulingapplication.ConsultantScheduleReportController">
    <children>
        <TableView fx:id="appointmentTableView" layoutX="16.0" layoutY="211.0" prefHeight="251.0" prefWidth="683.0">
            <columns>
                <TableColumn fx:id="DescriptionColumn" prefWidth="215.0" text="Appointment Description">
                    <cellValueFactory>
                        <PropertyValueFactory property="description" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="ApptStartTimeColumn" minWidth="0.0" prefWidth="148.0" text="Appointment Start Time">
                    <cellValueFactory>
                        <PropertyValueFactory property="apptStartTime" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="ApptStopTimeColumn" prefWidth="143.0" text="Appointment Stop Time">
                    <cellValueFactory>
                        <PropertyValueFactory property="apptStopTime" />
                    </cellValueFactory>
                </TableColumn>
                <TableColumn fx:id="LocationColumn" prefWidth="176.0" text="Location">
                    <cellValueFactory>
                        <PropertyValueFactory property="location" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
        <Label layoutX="14.0" layoutY="20.0" text="Consultant Schedule">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TableView fx:id="contactTableView" layoutX="192.0" layoutY="11.0" prefHeight="193.0" prefWidth="194.0">
            <columns>
                <TableColumn fx:id="ContactColumn" prefWidth="191.0" text="Contact">
                    <cellValueFactory>
                        <PropertyValueFactory property="contact" />
                    </cellValueFactory>
                </TableColumn>
            </columns>
        </TableView>
        <Button fx:id="showContactsBtn" layoutX="29.0" layoutY="81.0" mnemonicParsing="false" onAction="#showContactsBtnHandler" prefHeight="27.0" prefWidth="117.0" text="Show Consultants" />
        <ComboBox fx:id="contactBox" layoutX="440.0" layoutY="82.0" prefWidth="150.0" />
    </children>
</AnchorPane>
